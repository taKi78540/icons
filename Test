Anciennete_Coach_Active :=
VAR CoachesActifs =
    FILTER (
        Coaches_par_mois,
        CALCULATE (
            COUNTROWS ( 'lecomptoir allvisits_181124' ),
            'lecomptoir allvisits_181124'[Coach] = Coaches_par_mois[Coach]
        ) > 0
    )
RETURN
AVERAGEX (
    CoachesActifs,
    DATEDIFF (
        Coaches_par_mois[Premier_mois_coach],
        Coaches_par_mois[Premi√®re_Date],
        MONTH
    )
)
