EntitesEtPourcentages = 
VAR TotalComptoir = 
    CALCULATE(
        SUM('TableVisites'[Cout]),
        ALLEXCEPT('TableVisites', 'TableVisites'[Comptoir])
    )

RETURN
CONCATENATEX(
    VALUES('TableVisites'[VisitorType]),
    'TableVisites'[VisitorType] & " (" & 
    FORMAT(
        DIVIDE(
            CALCULATE(SUM('TableVisites'[Cout])),
            TotalComptoir,
            0
        ),
        "0%"
    ) & ")",
    ", ",
    'TableVisites'[VisitorType]
)
