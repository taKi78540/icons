javascript:(function(){
  var select=document.querySelector('select[name="param[201][2]"]');
  if(select){
    select.value="non";
    select.dispatchEvent(new Event('change',{bubbles:true}));
    alert('✅ Valeur mise sur "non"');
  } else {
    alert('⚠️ Champ non trouvé sur cette page');
  }
})();
