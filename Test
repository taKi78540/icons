MoyenneCV_VisitesParCoachParJour = 
AVERAGEX(
    VALUES('lecomptoir allvisits_181124'[CreatedAt].[Date]),
    VAR CoachsJour = 
        ADDCOLUMNS(
            VALUES('lecomptoir allvisits_181124'[coach.id]),
            "NbVisites", 
            CALCULATE(
                COUNTROWS('lecomptoir allvisits_181124')
            )
        )
    VAR Moyenne = AVERAGEX(CoachsJour, [NbVisites])
    VAR EcartType = STDEVX.P(CoachsJour, [NbVisites])
    RETURN DIVIDE(EcartType, Moyenne)
)
