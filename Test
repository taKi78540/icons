Tickets_traite_mois =
VAR CoachId = Coaches_par_mois[coach.id]
VAR DebutMois = STARTOFMONTH ( Coaches_par_mois[Date] )
VAR FinMois = ENDOFMONTH ( Coaches_par_mois[Date] )
RETURN
CALCULATE (
    COUNTROWS ( 'lecomptoir allvisits_181124' ),
    'lecomptoir allvisits_181124'[coach.id] = CoachId,
    'lecomptoir allvisits_181124'[CreatedAt] >= DebutMois,
    'lecomptoir allvisits_181124'[CreatedAt] <= FinMois
)
